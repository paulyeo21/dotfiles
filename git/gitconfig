[user]
	name = Paul Yeo
	email = kwangupaulyeo@gmail.com
[core]
	editor = vim
	excludesfile = /Users/paulyeo/.gitignore_global
[filter "lfs"]
	required = true
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
[alias]
	sla = log --oneline --decorate --graph --all
	glog = log -E -i --grep
	unstage = reset
	uncommit = reset --soft HEAD^
	aa = add --all
	co = checkout
	slo = log --oneline --decorate
	delr = push origin --delete
	fo = fetch origin
  mup = !git checkout master && git pull && git checkout -
  ups = rev-parse --abbrev-ref --symbolic-full-name @{upstream}
  del = branch -d
  rename = branch -m
  unset = branch --unset-upstream
  pru = remote prune origin
[push]
	default = upstream
